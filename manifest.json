{
  "manifest_version": 3,
  "name": "lower",
  "description": "make the web lower their voice a little",
  "version": "1",
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "content.css",
        "icon-128.png"
      ],
      "matches": []
    }
  ]
}
